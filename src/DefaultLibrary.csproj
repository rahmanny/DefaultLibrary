<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Nullable>enable</Nullable>
        <TargetFrameworks>net7.0;net6.0;net8.0</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>0.0.12.0</Version>
        <AssemblyVersion>0.0.12.9407</AssemblyVersion>
        <FileVersion>0.0.12.9407</FileVersion>
        <PackageProjectUrl>https://github.com/rahmanny/DefaultLibrary</PackageProjectUrl>
        <Copyright>Alexandr Rahmanny</Copyright>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Authors>Alexandr Rahmanny</Authors>
        <RepositoryUrl>https://github.com/rahmanny/DefaultLibrary</RepositoryUrl>
        <Title>Default Library</Title>
        <Description>Abstract Class Library</Description>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
    </PropertyGroup>

    <ItemGroup>
      <None Include="..\README.md">
        <Pack>True</Pack>
        <PackagePath>\</PackagePath>
      </None>
    </ItemGroup>

</Project>
